class Main {
    public static void main(String[] args) {
//Question 1
    int d = 4;
    System.out.println(d++ + ++d +d);
//Question 2
    int a = 2;
    int b = 3;
    boolean result =(a++ == 2)&&(++b == 4);
    System.out.println("a="+a+" b="+b+",result="+result);
//Question 3
    int x=5;
    x*=x+2;
    System.out.println(x);
//Questin 4
    int y =7;
    y = y++ * y-- + ++y;
    System.out.println(y);
//Question 5
    char ch = 'B';
    System.out.println((int)ch+3);
    
    
    }
}
